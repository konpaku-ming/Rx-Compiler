fn main() {
    // Print a header value
    printInt(0);
    
    // Read input
    let input: i32 = getInt();
    
    // Process the input
    let result: i32 = input + 10;
    
    // Print results using both functions
    printInt(result);
    printlnInt(result);
    
    // Test in a loop
    let mut i: i32 = 0;
    while (i < 3) {
        printlnInt(i);
        i += 1;
    }
    
    exit(0);
}
