fn sum_n(n: i32) -> i32 {
    let mut s: i32 = 0;
    let mut i: i32 = 1;
    while ((i <= n)) {
        s = s + i;
        i = i + 1;
    }
    return s;
}

fn main() {
    let res: i32 = sum_n(100);
    printInt(res);
    exit(0);
}
