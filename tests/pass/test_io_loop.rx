fn main() {
    let n: i32 = getInt();
    let mut sum: i32 = 0;
    let mut i: i32 = 0;
    
    while (i < n) {
        let val: i32 = getInt();
        sum += val;
        printInt(sum);
        i += 1;
    }
    
    printlnInt(sum);
    exit(0);
}
