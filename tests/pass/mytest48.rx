fn fact(n: i32) -> i32 {
    let mut res: i32 = 1;
    let mut i: i32 = 2;
    while ((i <= n)) {
        res = res * i;
        i = i + 1;
    }
    return res;
}

fn main() {
    let f: i32 = fact(6);
    printInt(f);
    exit(0);
}
